{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "_id": {
                "type": "string",
                "format": "uuid",
                "coerce": true
            },
            "marketTrend": {
                "type": "string"
            },
            "earthStatus": {
                "type": "string"
            },
            "marsStatus": {
                "type": "string"
            },
            "venusStatus": {
                "type": "string"
            },
            "collectiveGoal": {
                "type": "string"
            }
        },
    "required": ["_id", "marketTrend", "earthStatus", "marsStatus", "venusStatus", "collectiveGoal"],
    "additionalProperties": false
    }
}